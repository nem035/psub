{
  "name": "psub",
  "author": "Nemanja Stojanovic",
  "description": "Publish/Subscribe in JavaScript (ES6)",
  "main": "dist/index.js",
  "version": "0.9.2",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js",
    "lint": "eslint . && echo All good!",
    "prebuild": "rimraf build",
    "precommit": "npm run validate",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "nyc mocha --compilers js:babel-core/register",
    "validate": "npm-run-all --parallel test lint build",
    "watch:test": "mocha --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nem035/psub.git"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.0.15",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.0.15",
    "babel-preset-babili": "^0.0.9",
    "babili-webpack-plugin": "^0.0.7",
    "codecov": "^1.0.1",
    "eslint": "^3.13.1",
    "eslint-config-google": "^0.7.1",
    "expect": "^1.20.2",
    "husky": "^0.12.0",
    "mocha": "^3.2.0",
    "mocha-eslint": "^3.0.1",
    "npm-run-all": "^4.0.0",
    "nyc": "^10.0.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.14.0"
  },
  "keywords": [
    "publish",
    "subscribe",
    "design",
    "pattern",
    "javascript"
  ],
  "nyc": {
    "all": true,
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "include": [
      "src"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  },
  "dependencies": {}
}
