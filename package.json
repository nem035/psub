{
  "name": "psub",
  "author": "Nemanja Stojanovic",
  "description": "PublishSubscribe pattern in ES6 (ECMAScript2015)",
  "main": "src/index.js",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "babili src -d dist --no-comments",
    "lint": "eslint . && echo All good!",
    "test": "nyc mocha --compilers js:babel-core/register",
    "watch:test": "mocha --watch"
  },
  "devDependencies": {
    "babel-core": "^6.0.15",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.0.15",
    "babili": "^0.0.9",
    "eslint": "^3.13.1",
    "eslint-config-google": "^0.7.1",
    "expect": "^1.20.2",
    "mocha": "^3.2.0",
    "mocha-eslint": "^3.0.1",
    "nyc": "^10.0.0"
  },
  "keywords": [
    "publish",
    "subscribe",
    "design",
    "pattern",
    "javascript"
  ],
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "reporter": [
      "text",
      "lcov"
    ]
  }
